

<table class="ui celled table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Activité</th>
      <th>Etat theorique</th>
      <th>Categorie</th>
      <th>Niveau de défi</th>
      <th>Niveau de compétences</th>
      <th>Compétent</th>
      <th>Absorbé</th>
      <th>Objectifs clairs</th>
      <th>Controle</th>
      <th>Libre de l'ego</th>
      <th>Libbre du temps</th>
      <th>Conditions réussite/échec claires</th>
      <th>Expérience enrichissante</th>
      <th>De mon initiative</th>
      <th>Niveau de bonheur</th>
      <th>Ambiance sociale</th>
    </tr>
  </thead>
  <tbody>
		<% @flow_entries.each do |flow_entry| %>
    	<tr>
				<td><%= flow_entry.created_at.to_formatted_s(:short)  %></td>
				<td><%= flow_entry.activity_name %></td>
				<td><%= flow_entry.theorical_state %></td>
				<td><%= flow_entry.activity_category %></td>
				<td><%= flow_entry.challenge_level + 1 %></td>
				<td><%= flow_entry.skill_level + 1 %></td>
				<td><%= flow_entry.feeling_competent + 1 %></td>
				<td><%= flow_entry.mentally_absorbed + 1 %></td>
				<td><%= flow_entry.clear_objectives + 1 %></td>
				<td><%= flow_entry.control_situation + 1 %></td>
				<td><%= flow_entry.free_from_ego + 1 %></td>
				<td><%= flow_entry.free_from_time + 1 %></td>
				<td><%= flow_entry.clear_conditions + 1 %></td>
				<td><%= flow_entry.great_experience + 1 %></td>
				<td><%= flow_entry.is_voluntary? "Oui" : "Non" %></td>
				<td><%= flow_entry.happiness_level + 1%></td>
				<td><%= flow_entry.social_quality %></td>
			</tr>
		<% end %>
  </tbody>
</table>
